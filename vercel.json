{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/admin",
      "destination": "/api/admin"
    },
    {
      "source": "/admin/:path*",
      "destination": "/api/admin"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
